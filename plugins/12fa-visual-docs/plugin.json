        "svg": "dot -Tsvg diagram.dot -o diagram.svg",
        "png": "dot -Tpng diagram.dot -o diagram.png"
      },
      "batch_render": {
        "all_skills": "cd docs/workflows/graphviz/skills && for f in *.dot; do dot -Tsvg \"$f\" -o \"${f%.dot}.svg\"; done",
        "all_diagrams": "cd docs/workflows/graphviz && for dir in skills agent-mappings; do (cd $dir && for f in *.dot; do dot -Tsvg \"$f\" -o \"${f%.dot}.svg\"; done); done"
      }
