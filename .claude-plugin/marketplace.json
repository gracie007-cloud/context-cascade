  "quick_start": {
    "install_marketplace": "/plugin marketplace add DNYoussef/context-cascade",
    "install_core": "/plugin install context-cascade-core",
    "install_all": "/plugin install context-cascade-core context-cascade-three-loop context-cascade-security context-cascade-visual-docs context-cascade-swarm"
  },
  "mcp_servers": {
    "required": {
      "claude-flow": {
        "install": "npm install -g claude-flow@alpha",
        "add": "claude mcp add claude-flow npx claude-flow@alpha mcp start"
      }
    },
    "recommended": {
      "ruv-swarm": {
        "install": "npm install -g ruv-swarm",
        "add": "claude mcp add ruv-swarm npx ruv-swarm mcp start"
      },
      "flow-nexus": {
        "install": "npm install -g flow-nexus@latest",
        "add": "claude mcp add flow-nexus npx flow-nexus@latest mcp start"
      }
    }
  },
  "compatibility": {
    "claude_code_version": ">=2.0.13",
    "node_version": ">=18.0.0",
    "npm_version": ">=9.0.0"
